    "import numpy as np\n",
    "import math as mt\n",
    "\n",
    "from scipy.integrate import quad\n",
    "\n",
    "def integrand(x):\n",
    "    return (x**3 - (3/5)*x)**2\n",
    "\n",
    "result, error = quad(integrand, -1, 1)\n",
    "print(f\"Numerical result: {result}\")\n",
    "print(f\"Exact fraction: {result.as_integer_ratio()}\")"
